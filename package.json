{
  "name": "@xiroex/firebase-admin",
  "version": "0.0.3",
  "description": "A simplified wrapper for Firebase Admin SDK",
  "author": "George Abreu Jr. <george8794@gmail.com> (https://georgeanthony.net/)",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/xiroex/firebase-admin#readme",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "tsc -d"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "admin",
    "database",
    "Firebase",
    "gcp",
    "firestore"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xiroex/firebase-admin.git"
  },
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "firebase-admin": "^12.1.0",
    "uuid": "^9.0.1"
  },
  "bugs": {
    "url": "https://github.com/xiroex/firebase-admin/issues"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.0.0",
    "typescript": "^5.4.5"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "firestore": [
        "dist/lib/firestore.d.ts"
      ],
      "user": [
        "dist/lib/user.d.ts"
      ],
      "authentication": [
        "dist/lib/authentication.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./firestore": {
      "types": "./dist/lib/firestore.d.ts",
      "require": "./dist/lib/firestore.js",
      "import": "./dist/lib/firestore.js"
    },
    "./user": {
      "types": "./dist/lib/user.d.ts",
      "require": "./dist/lib/user.js",
      "import": "./dist/lib/user.js"
    },
    "./authentication": {
      "types": "./dist/lib/authentication.d.ts",
      "require": "./dist/lib/authentication.js",
      "import": "./dist/lib/authentication.js"
    }
  }
}
